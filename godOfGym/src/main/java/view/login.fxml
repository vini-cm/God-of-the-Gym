<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="355.0" styleClass="anchorPane" stylesheets="@../../resources/css/login.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <TextField fx:id="tfNome" layoutX="23.0" layoutY="250.0" prefHeight="45.0" prefWidth="320.0" promptText="NOME" styleClass="textField" />
      <PasswordField fx:id="password" layoutX="23.0" layoutY="309.0" prefHeight="45.0" prefWidth="320.0" promptText="SENHA" styleClass="textField" />
      <Button fx:id="bntEntrar" layoutX="23.0" layoutY="375.0" mnemonicParsing="false" onAction="#onClickLogar" prefHeight="56.0" prefWidth="320.0" styleClass="btnEntrar" stylesheets="@../../resources/css/login.css" text="ENTRAR" textFill="#d90429">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <ImageView fitHeight="225.0" fitWidth="298.0" layoutX="34.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/imagens/logo.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
