      <TextField fx:id="tfNome" layoutX="175.0" layoutY="42.0" promptText="Nome" visible="false" />
      <ChoiceBox fx:id="cbTipo" layoutX="175.0" layoutY="75.0" prefWidth="150.0" visible="false" />
